    "vuetify": "2.6.11",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.3",
    "@fortawesome/fontawesome-free": "6.2.0",
    "@mdi/font": "7.0.96",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
